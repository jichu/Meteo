<UserControl x:Class="MeteoViewer.TreeView.UserControlTree"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MeteoViewer.TreeView"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="220" Loaded="UserControl_Loaded">
    <Grid>
        <TreeView Name="TreeJson" HorizontalAlignment="Left" Height="430" Margin="10,40,0,0" VerticalAlignment="Top" Width="200" SelectedItemChanged="TreeJson_SelectedItemChanged"/>
        <Label Content="export" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" MouseDoubleClick="Label_MouseDoubleClick"/>
    </Grid>
</UserControl>
